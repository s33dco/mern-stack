{
	"name": "webpack-for-react",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "https://github.com/s33dco/mern-stack.git",
	"author": "s33dco <code@s33d.co>",
	"license": "MIT",
	"private": true,
	"config": {
		"webpack_args": "webpack --config webpack.config.js --display-error-details"
	},
	"scripts": {
		"dev": "concurrently \"yarn dev:start\" \"webpack-dev-server\"",
		"dev:prebuild": "rimraf dev",
		"dev:build": "cross-env NODE_ENV=development $npm_package_config_webpack_args",
		"prebuild": "rimraf build",
		"build": "cross-env NODE_ENV=production $npm_package_config_webpack_args --env=prod",
		"start": "node server/server.js",
		"dev:start": "nodemon server/server.js",
		"runinprod": "concurrently \"cross-env NODE_ENV=production yarn build\" \" cross-env NODE_ENV=production yarn start\"",
		"test": "jest --watchAll --coverage --runInBand"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"config": "^3.2.2",
		"express": "^4.17.1",
		"express-validator": "^6.1.1",
		"helmet": "^3.20.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.24.0",
		"mongodb": "^3.3.0-beta2",
		"mongoose": "^5.6.7",
		"morgan": "^1.9.1",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-imported-component": "^5.5.3",
		"react-moment": "^0.9.2",
		"react-redux": "^7.1.0",
		"react-router-dom": "^5.0.1",
		"react-transition-group": "^4.2.1",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8",
		"redux-thunk": "^2.3.0",
		"winston": "^3.2.1",
		"winston-daily-rotate-file": "^3.10.0",
		"winston-mongodb": "^5.0.0"
	},
