		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"concurrently": "^4.1.1",
		"copy-webpack-plugin": "^5.0.4",
		"cross-env": "^5.2.0",
		"css-loader": "^3.1.0",
